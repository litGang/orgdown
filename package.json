{
  "name": "orgdown-notebook",
  "version": "1.0.0",
  "description": "orgdown markdown editor notebook project.",
  "main": "src/electron/bootstrap.js",
  "scripts": {
    "start": "webpack-dev-server --hot --progress --colors ",
    "build": "webpack --progress --colors",
    "orgdown": "npm run hot && npm run electron",
    "electron": "HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./src/electron/bootstrap",
    "hot": "NODE_ENV=development node --max_old_space_size=2096 -r babel-register server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shalir/orgdown.git"
  },
  "keywords": [
    "orgdown",
    "markdown",
    "notebook",
    "note"
  ],
  "author": "wangyg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shalir/orgdown/issues"
  },
  "homepage": "https://github.com/shalir/orgdown#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.0",
    "file-loader": "^0.11.1",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "nedb": "^1.8.0",
    "prop-type": "0.0.1",
    "react": "^15.5.4",
    "react-ace": "^4.2.1",
    "react-dom": "^15.5.4"
  }
}
